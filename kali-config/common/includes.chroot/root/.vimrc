filetype plugin indent on

set nowrap

set noswapfile

set hlsearch
set ignorecase
set smartcase

set autoindent

set ruler
set number
set list
set wildmenu
set showcmd

set shiftwidth=4
set softtabstop=4
set expandtab
set tabstop=4
set smarttab

set clipboard=unnamed

"""""""""""""""""""""""""""""""""""""""
"Key Map Setting
"""""""""""""""""""""""""""""""""""""""
imap ( ()<left>
imap [ []<left>
imap { {}<left>
imap < <><left>
inore ' ''<left>
inore " ""<left>
